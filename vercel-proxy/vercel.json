{
  "version": 2,
  "builds": [
    {
      "src": "api/token.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/update-leaderboard.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/register-tournament.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token.js"
    },
    {
      "src": "/api/update-leaderboard",
      "dest": "/api/update-leaderboard.js"
    },
    {
      "src": "/api/register-tournament",
      "dest": "/api/register-tournament.js"
    }
  ]
}
